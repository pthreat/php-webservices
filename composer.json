{
    "name": "PHP Webservices REST API",
    "author": "Federico Stange <jpfstange@gmail.com>",
    "description": "Slim Framework skeleton with custom dependencies and a nifty install script",
    "keywords": [
        "microframework",
        "rest",
        "router",
        "psr7"
    ],
    "homepage": "http:\/\/github.com\/pthreat\/php-webservices",
    "license": "MIT",
    "require": {
        "php": ">=5.5.0",
        "slim\/slim": "^3.1",
        "odan\/slim-session": "^1.0",
        "monolog\/monolog": "^1.17"
    },
    "require-dev": {
        "propel\/propel": "2.0.0-alpha8"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests\/"
        }
    },
    "config": {
        "process-timeout": 0
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }
}
